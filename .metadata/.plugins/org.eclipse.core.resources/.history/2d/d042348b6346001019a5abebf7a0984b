package com.kopo.lab0611;

public class User {
	int idx;
	String id;
	String pwd;
	String name;
	String phone;
	String address;
	String created;
	String lastUpdated;

	User() {

	}

	User(int idx, String id, String pwd, String name, String phone, String address, String created,
			String lastUpdated) {
		this.idx = idx;
		this.id = id;
		this.pwd = pwd;
		this.name = name;
		this.phone = phone;
		this.address = address;
		this.created = created;
		this.lastUpdated = lastUpdated;
	}

	User(String id, String pwd) {
		this.id = id;
		this.pwd = pwd;
	}

	User(String id, String pwd, String name, String phone, String address) {
		this.id = id;
		this.pwd = pwd;
		this.name = name;
		this.phone = phone;
		this.address = address;
	}

	public int getIdx() {
		return this.idx;
	}

	public String getId() {
		return this.id;
	}

}
