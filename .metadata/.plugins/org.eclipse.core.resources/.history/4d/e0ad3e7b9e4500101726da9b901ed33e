package com.kopo.lab0610;

public class StudentRankDTO {
	private String name;
	private double midScore;
	private double finScore;
	private int rank;

	public StudentRankDTO(String name, double midScore, double finScore) {
		this.name = name;
		this.midScore = midScore;
		this.finScore = finScore;
		this.rank = rank;
	}

	public String getName() {
		return name;
	}

	public double getMidScore() {
		return midScore;
	}

	public double getFinScore() {
		return finScore;
	}
	
	public int getRank() {
		return rank;
	}
}
